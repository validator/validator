  <a:documentation>
    use: Use Element
  </a:documentation>

  <define name="SVG.use.content">
    <zeroOrMore>
      <ref name="SVG.Description.class"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="SVG.Animation.class"/>
    </zeroOrMore>
  </define>

  <define name="use">
